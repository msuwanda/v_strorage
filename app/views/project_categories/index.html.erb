<h1>Listing project_categories</h1>

<table>
  <thead>
    <tr>
      <th>Hashtag</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_categories.each do |project_category| %>
      <tr>
        <td><%= project_category.hashtag %></td>
        <td><%= link_to 'Show', project_category %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

